<view class="watermarks" style="{{'opacity:'+(opacity)+';'+('height:'+(height)+';')+('position:'+(position)+';')+('bottom:'+(bottom)+';')+('z-index:'+(zindex)+';')}}"><view class="watermarksshell"><block wx:for="{{count}}" wx:for-item="t" wx:for-index="i"><block><view style="{{'transform:'+('rotate('+rotate+'deg)')+';'}}"><block wx:if="{{logo!=''||logo!='none'}}"><image style="{{'width:'+(size*texts.length+'em')+';'+('height:'+(size*texts.length+'em')+';')+('float:'+('left')+';')}}" src="{{logo}}"></image></block><view style="{{'padding-left:'+(logo!=''||logo!='none'?size*texts.length+0.2+'em':0+'em')+';'}}"><block wx:for="{{texts}}" wx:for-item="text" wx:for-index="c"><view style="{{'color:'+(color)+';'+('font-size:'+(size+'em')+';')+('line-height:'+(size+'em')+';')}}">{{text}}</view></block></view></view></block></block></view></view>